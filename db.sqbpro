olumn_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="App_Order_cart" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="49"/><column index="2" value="221"/><column index="3" value="78"/><column index="4" value="79"/><column index="5" value="61"/><column index="6" value="64"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="App_Order_order" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="49"/><column index="2" value="221"/><column index="3" value="61"/><column index="4" value="87"/><column index="5" value="64"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="App_Shop_product" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="49"/><column index="2" value="300"/><column index="3" value="188"/><column index="4" value="300"/><column index="5" value="300"/><column index="6" value="57"/><column index="7" value="71"/><column index="8" value="221"/><column index="9" value="91"/><column index="10" value="106"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">-- FIRST query : display all items bought by customer id 3 
SELECT CART.product_id , PROD.name
FROM App_Login_customer CUST , App_Order_cart CART , App_Order_order ORD , App_Shop_product PROD 
WHERE CUST.id = CART.user_id
		AND CUST.id = ORD.user_id
		AND CART.product_id = PROD.id
		--AND ORD.ordered=1
		AND CUST.id = 3 ;


-- SECOND query : Display the number of orders ( sales made) for each product
SELECT PROD.name , COUNT(CART.id) AS SALES
FROM App_Order_cart CART , App_Shop_product PROD 
WHERE CART.product_id = PROD.id 
GROUP BY PROD.name ;


-- THIRD QUERY : Display total revenue generated by each seller
SELECT SELLER.full_name , SUM(PRODUCT.price) AS TOTAL_REVENUE
FROM App_Login_seller SELLER , App_Order_cart CART , App_Shop_product PRODUCT 
WHERE SELLER.id = PRODUCT.added_by_id
AND CART.product_id = PRODUCT.id
GROUP BY SELLER.full_name ;


-- FOURTH QUERY: TOTAL money spent by each customer
SELECT CUSTOMER.id , SUM(PRODUCT.price) AS MONEY_SPENT
FROM App_Login_customer CUSTOMER , App_Order_cart CART , App_Shop_product PRODUCT 
WHERE CUSTOMER.id = CART.user_id
AND CART.product_id = PRODUCT.id
GROUP BY CUSTOMER.id ;


-- FIFTH QUERY: name and id of seller with more than 1 customer

SELECT SELLER.id , SELLER.full_name
FROM App_Login_seller SELLER
WHERE 1 &lt; ( SELECT COUNT(CART.user_id) 
FROM App_Order_cart CART 
GROUP BY CART.user_id )

GROUP BY SELLER.id ;
</sql><current_tab id="0"/></tab_sql></sqlb_project>
